onse.json()\n\n        const dataArr = []\n\n        for (let id in data){\n          dataArr.push(data[id])\n        }\n\n        commit('cargar', dataArr)\n\n      } catch (error) {\n        console.error(error)\n      }\n    },\n\n    // Create task\n    async setTasks({commit}, task){\n      try {\n        \n        const response = await fetch(`https://vue-firebase-api-357ef-default-rtdb.firebaseio.com/task/${task.id}.json`,{\n          method:'PUT',\n          headers:{\n            'Content-Type':'application.json'\n          },\n          body: JSON.stringify(task)\n        })\n\n        const data = await response.json()\n        console.log(data);\n      } catch (error) {\n        console.error(error)\n      }\n\n      \n\n      commit('set', task)\n    },\n    // Delete task\n    async deleteTasks({commit}, id){\n\n        await fetch(`https://vue-firebase-api-357ef-default-rtdb.firebaseio.com/task/${id}.json`,{\n          method:'DELETE'\n        })\n        commit('delete', id)\n    },\n    // Edit task\n    editTasks({commit}, id){\n      commit('edit', id)\n    },\n    // Update task\n    async updateTask({commit}, task){\n\n      try {\n        const response = await fetch(`https://vue-firebase-api-357ef-default-rtdb.firebaseio.com/task/${task.id}.json`,{\n          method:'PATCH',\n          body: JSON.stringify(task)\n        })\n\n        const data = await response.json()\n\n        commit('update', data)\n      } catch (error) {\n        console.error(error)\n      }\n\n      \n    }\n  },\n  modules: {\n  }\n})\n"]}]}